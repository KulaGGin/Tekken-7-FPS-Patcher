<Application x:Class="FPSPatcher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FPSPatcher"
             xmlns:ViewModels="clr-namespace:FPSPatcher.ViewModels;assembly=FPSPatcher.ViewModels"
             Startup="App_OnStartup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="DataTemplatesDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <ViewModels:MainViewModel x:Key="MainViewModel"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
