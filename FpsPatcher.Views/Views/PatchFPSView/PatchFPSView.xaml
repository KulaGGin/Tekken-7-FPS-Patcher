<UserControl x:Class="FpsPatcher.Views.PatchFpsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FpsPatcher.Views"
             mc:Ignorable="d">
    <UserControl.Resources>
        <ResourceDictionary><ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="PatchFPSViewDictionary.xaml" />
        </ResourceDictionary.MergedDictionaries></ResourceDictionary>
    </UserControl.Resources>
    <Grid Margin="10">
        <CheckBox x:Name="OneFrameThreadLagCheckBox" Content="r.OneFrameThreadLag" HorizontalAlignment="Left" Margin="0,28,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="FpsTextBox" HorizontalAlignment="Left" Height="23" Margin="0,0,0,0" TextWrapping="Wrap" Text="120" VerticalAlignment="Top" Width="137"/>
        <Button x:Name="PatchButton" Content="Patch" HorizontalAlignment="Left" Margin="0,68,0,0" VerticalAlignment="Top" Width="137" />
    </Grid>
</UserControl>
