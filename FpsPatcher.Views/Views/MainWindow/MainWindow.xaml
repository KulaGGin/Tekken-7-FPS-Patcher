<Window x:Class="FPSPatcher.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FPSPatcher.Views"
        xmlns:ViewModels="clr-namespace:FPSPatcher.ViewModels;assembly=FPSPatcher.ViewModels"
        xmlns:Views="clr-namespace:FPSPatcher.Views"
        mc:Ignorable="d"
        Title="Tekken 7 FPS Patcher" SizeToContent="WidthAndHeight" ResizeMode="NoResize"
        Icon="/FPSPatcher.Views;component/Views/MainWindow/tekken-7-icon.png">
    <Window.Resources>
        <ViewModels:PatchFPSViewModel x:Key="PatchFPSViewModel"/>
    </Window.Resources>
    <Grid>
        <ContentControl Content="{StaticResource PatchFPSViewModel}" />
    </Grid>
</Window>
